    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.10.0",
    "typescript": "*"
  },
  "dependencies": {
    "@types/node-localstorage": "^1.3.0",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
